function void fibonacci(int num)
{
  int i, j, t;
  j = 1;
  i = 0;
  t = 1;
  
  if (num < 0)
  {
      put("%s", "Não é possível fazer uma sequencia fibonacci");
  }
  
  else
  {
      put("%d", 0);
      while (t <= num)
          {
              put(", %d", t);
              t = i + j;
              i = j;
              j = t;
              
          }
      
      
      
  }
 
}

void begin()
{
   int num;
   read("%d", &num);
   fibonacci(num);
}
