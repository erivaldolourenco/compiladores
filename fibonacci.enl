
function void fibonacci(int num)
{
  int i, j, t, k;
  j = 1;
  k = 1;
  i = 0;
  
  if (num <= 0)
  {
      put('%s', 'Não é possível fazer uma sequencia fibonacci');
  }

  else
  {
      put('%d', 1);

      while(k <= num)
      {
           t = i + j;
           i = j;
           j = t;
      
           if(j <= num)
           {
               put(', %d', j);
           }
           k = k + 1;
      }
  }
 
}

void main()
{
   int num;
   read('%d', num);
   fibonacci(num);
}
