Programa = Funcao Begin
Funcao = 'function' Vector Tipo NomeVar 'abrPar' LParam 'fecPar' Escopo
Funcao = ε
Begin = 'void' 'begin' 'abPar' LParam 'fecPar' Escopo

NomeVar = 'id' VectorF
Vector = 'vector' VectorF
Vector = ε
VectorF = 'abrCoc' 'CTEINT' 'feccoc'
Vectorf = ε

LParam = Vector Tipo NomeVar LParamF
LParam = ε
LParamF = 'sepVirg' lparam
LParamF = ε


Tipo = 'int'
Tipo = 'float'
Tipo = 'cchar'
Tipo = 'char'
Tipo = 'bool'
Tipo = 'void'


Escopo = 'abrCh' Comandos 'fecCh'
Comandos = Cmd Comandos
Comandos = ε
Cmd = Read
Cmd = Put
Cmd = While
Cmd = If
Cmd = For
Cmd = VectorDecl 'SepPtv'
Cmd = Tipo Decl 'SepPtv'
