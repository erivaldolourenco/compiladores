Cmd = 'return' E 'sepPtv'

Decl = NomeVar AtbDecl
VectorDecl = 'vector' tipo decl
ChamadaFunc = 'abrPar' LChamada 'fecPar'
LChamada = E LChamadaF
LChamada = ε
LChamadaF = 'SepVirg' E LChamadaF
LChamadaF = ε

AtbDecl = 'atribuicao' E
AtbDecl = ε
Read = 'read' //corrigir
Put = 'put' //corrigir


CmdF = Atribuicao
CmdF = ChamadaFunc
CmdF = 'abrCoc' E 'fecCoc' Atribuicao
Atribuicao = 'atribuicao' E
While = 'while' 'abPar' E 'fecPar' Escopo
If = 'if' 'abPar' E 'fecPar' Escopo Else
Else = 'else'  Escopo
Else = ε
For = 'for' NomeVar 'in' E 'to' E 'do' Escopo
E = Eb Ebr
Eb = Tb Ebr
Ebr = 'opOr' Tb Ebr
Ebr = ε
Tb = Fb Tbr
Tbr = 'opAnd' Fb Tbr
Tbr = ε
Fb = 'opNegac' ERelUm
Fb = ERelUm
RelUm = ERelDois ERelUmF
RelUmF = 'opRel1' ERelDois
RelUmF = ε
RelDois = EConcat ERelDoisF
RelDoisF = 'opRel2' EConcat
RelDoisF = ε
