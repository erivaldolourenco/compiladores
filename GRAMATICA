DeclF = ε
AtbDecl = 'atrib' E
AtbDecl = ε
Read = 'read' 'abPar' NomeVar 'fecPar' 'sepPtv'
Put = 'put' 'abPar' LChamada 'fecPar' 'sepPtv'

