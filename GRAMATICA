LparamF = ε

Tipo = 'int'
Tipo = 'float'
Tipo = 'cchar'
Tipo = 'char'
Tipo = 'bool'
Tipo = 'void'

Array = 'abCol' ArrayF
ArrayF = E 'fecCol'
ArrayF = 'fecCol'
Array = ε
NomeVar = 'id' Array

ChamadaFunc = 'abrPar' LChamada 'fecPar'
